    διαδικασία factor(n)
    	διαπροσωπεία
    	είσοδος n
    	δήλωση temp
    	αρχή_διαδικασίας
    		γράψε n;

    		εάν n = 0 τότε
    			f := 1
    		αλλιώς
    			temp := n - 1;
    			εκτέλεσε factor(temp);
    			f := f * n
    		εάν_τέλος
